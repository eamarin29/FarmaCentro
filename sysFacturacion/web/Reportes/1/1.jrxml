	<queryString>
		<![CDATA[SELECT ROWNUM TOTAL,
FA.TOTAL_VENTA,
CO.SALDO
FROM FACTURA FA, COMISION CO
WHERE FA.FECHA_REGISTRO BETWEEN $P{fecha_ini} AND $P{fecha_fin}
AND FA.CODVENDEDOR = $P{codVendedor}
AND FA.ANULADA='N'
AND FA.CODFACTURA=CO.FACTURA]]>
	</queryString>
