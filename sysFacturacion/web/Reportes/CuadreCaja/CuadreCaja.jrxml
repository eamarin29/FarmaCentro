	<parameter name="codVendedor" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_ini" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_fin" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT FA.*,
(SELECT SUM(CO.SALDO)
FROM COMISION CO
WHERE CO.FECHA BETWEEN $P{fecha_ini} AND $P{fecha_fin}
AND CO.VENDEDOR = $P{codVendedor}) AS SALDO,
(SELECT SUM(FA.TOTAL_VENTA)
FROM FACTURA FA
WHERE FA.FECHA_REGISTRO BETWEEN $P{fecha_ini} AND $P{fecha_fin}
AND FA.CODVENDEDOR = $P{codVendedor}
AND FA.ANULADA='N') AS TOTALES_VENTAS,

(SELECT count(*)
FROM FACTURA FA
WHERE FA.FECHA_REGISTRO BETWEEN $P{fecha_ini} AND $P{fecha_fin}
AND FA.CODVENDEDOR = $P{codVendedor}
AND FA.ANULADA='N') AS CANTIDAD_FAC

FROM FACTURA FA
WHERE FA.FECHA_REGISTRO BETWEEN $P{fecha_ini} AND $P{fecha_fin}
AND FA.CODVENDEDOR = $P{codVendedor}
AND FA.ANULADA='N']]>
	</queryString>
